<launch>
  <param name="joy_node/dev" value="/dev/input/js0" />
  <node pkg="joy" type="joy_node"
    name="joy_node" output="screen" />

  <node pkg="open_quadruped" type="interface_process.py"
    name="interface_process" output="screen" />

  <node pkg="rosserial_python" type="serial_node.py"
    name="serial_node" output="screen">    
    <param name="port" value="/dev/rfcomm1" />
    <param name="baud" value="38400" />
  </node>
</launch>